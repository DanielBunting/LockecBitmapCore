<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Company />
    <Authors>Daniel Bunting</Authors>
    <PackageLicenseFile>License.txt</PackageLicenseFile>
    <RepositoryUrl>https://github.com/DanielBunting/LockedBitmap</RepositoryUrl>
    <PackageTags>LockedBitmap Fast Image Search</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>8.0.1</Version>
    <PackageProjectUrl>https://github.com/DanielBunting/LockedBitmap</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="8.0.4" />
  </ItemGroup>

  <ItemGroup>
    <None Update="License.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="License.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
